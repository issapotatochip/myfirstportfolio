from turtle import *
import math

# Name your Turtle.
t = Turtle()

# Set Up your screen and starting position.
setup(500,300)
x_pos = -250
y_pos = -150
t.setposition(x_pos, y_pos)

### Write your code below:
t.forward(400)
t.right(100)
t.right(112)
t.forward(300)






# Close window on click.
exitonclick()
